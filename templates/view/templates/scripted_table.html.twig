{% include 'view/templates/bare_table.html.twig'%}

<script>
		var timeout = null;
		function updateSearchString(e) {		
					clearTimeout(timeout);
					timeout = setTimeout(function () {
						showEntityList("{{ domain }}",e.target.name,document.getElementById('pgNum').innerHTML,e.target.value);
					}, 125);			

		}
		
		function TableClick(myobj, domain, name, instID, viewID,divID)
		{
			$(myobj).closest('table').find('td').not(this).removeClass('selected');      
			$(myobj).closest('table').find('label').not(this).removeClass('selected'); 
			$(myobj).parent().siblings().removeClass('highlight');
			$(myobj).parent().toggleClass('highlight');
			$(myobj).toggleClass('selected');
			showView(domain, name,instID,viewID,divID,-1);
		}
		
		function showEntityList(domain, name, page, searchstring) {
					showView(domain, name,'empty','View5d98dbc439a5a4.32197708','newtable',page);
		}
</script>